# ==========
#: ./data/scripts/player/background/simulation/supplycommand.lua:
msgid "Supplying Stations"
msgstr "Stazioni di rifornimento"

#: ./data/scripts/player/background/simulation/supplycommand.lua:
msgid ""
"Commander, we encountered an issue while supplying: Station '%s' has "
"disappeared!"
msgstr ""
"Comandante, abbiamo riscontrato un problema durante la fornitura: La "
"stazione '%s' è scomparsa!"

#: ./data/scripts/player/background/simulation/supplycommand.lua:
msgid ""
"Commander, we encountered an issue while supplying: Station %s can't trade "
"the goods!"
msgstr ""
"Comandante, abbiamo riscontrato un problema durante la fornitura: La "
"stazione %s non può scambiare la merce!"

#: ./data/scripts/player/background/simulation/supplycommand.lua:
msgid "goods"
msgstr "merci"

#: ./data/scripts/player/background/simulation/supplycommand.lua:
msgid "'%1%' delivered %2% %3% to station '%4%'."
msgstr "'%1%' ha consegnato %2% %3% alla stazione '%4%'."

#: ./data/scripts/player/background/simulation/supplycommand.lua:
msgid "Ship is supplying stations with goods."
msgstr "La nave rifornisce le stazioni di merci."

#: ./data/scripts/player/background/simulation/supplycommand.lua:
msgctxt "ship AI status"
msgid "Supplying stations"
msgstr "Stazioni di rifornimento"

#: ./data/scripts/player/background/simulation/supplycommand.lua:
msgid "We don't have enough cargo space to do that properly!"
msgstr "Non abbiamo abbastanza spazio di carico per farlo correttamente!"

#: ./data/scripts/player/background/simulation/supplycommand.lua:
msgid "There are no supply routes that we could fly!"
msgstr "Non ci sono rotte di rifornimento che possiamo percorrere!"

#: ./data/scripts/player/background/simulation/supplycommand.lua:
msgid "We need a route between two different sectors."
msgstr "Abbiamo bisogno di un percorso tra due settori diversi."

#: ./data/scripts/player/background/simulation/supplycommand.lua:
msgid "You don't have permission to manage alliance stations!"
msgstr "Non avete il permesso di gestire le stazioni dell'alleanza!"

#: ./data/scripts/player/background/simulation/supplycommand.lua:
msgid "Loop Time"
msgstr "Tempo di loop"

#: ./data/scripts/player/background/simulation/supplycommand.lua:
msgid ""
"We'll supply those stations, no problem. This shouldn't be a hard thing to "
"do."
msgstr ""
"Forniremo quelle stazioni, senza problemi. Non dovrebbe essere una cosa "
"difficile da fare."

#: ./data/scripts/player/background/simulation/supplycommand.lua:
msgid ""
"We will carry out the command until something goes wrong or you call us "
"back."
msgstr ""
"Eseguiremo il comando fino a quando qualcosa non andrà storto o ci "
"richiamerete."

#: ./data/scripts/player/background/simulation/supplycommand.lua:
msgid "I will supply the stations until you call me back."
msgstr "Fornirò le stazioni finché non mi richiamerete."

#: ./data/scripts/player/background/simulation/supplycommand.lua:
msgid "Supply Factories"
msgstr "Fabbriche di rifornimento"

#: ./data/scripts/player/background/simulation/supplycommand.lua:
msgid "Supply Operation"
msgstr "Operazione di approvvigionamento"

#: ./data/scripts/player/background/simulation/supplycommand.lua:
msgid "Free Cargo Space:"
msgstr "Spazio libero per il carico:"

#: ./data/scripts/player/background/simulation/supplycommand.lua:
msgid "Runtime:"
msgstr "Tempo di esecuzione:"

#: ./data/scripts/player/background/simulation/supplycommand.lua:
msgid "Indefinite"
msgstr "Indefinito"

#: ./data/scripts/player/background/simulation/supplycommand.lua:
msgid "${title} ${name}"
msgstr "${titolo} ${nome}"

#: ./data/scripts/player/background/simulation/supplycommand.lua:
msgctxt ""
"resolves to '41 Energy Cells' or '1 Mining Robot', plural form is already "
"taken care of here"
msgid "${amount} ${name}"
msgstr "${importo} ${nome}"

#: ./data/scripts/player/background/simulation/supplycommand.lua:
