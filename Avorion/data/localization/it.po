msgid "The Brotherhood"
msgstr "La Fratellanza"

#: ./data/scripts/lib/story/the4.lua:
msgid "Scanners online."
msgstr "Scanner online."

#: ./data/scripts/lib/story/the4.lua:
msgid "Looking for Xsotan Artifacts"
msgstr "Alla ricerca di manufatti Xsotan"

#: ./data/scripts/lib/story/the4.lua:
msgid "Hard"
msgstr "Duro"

